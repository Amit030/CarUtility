
abstract class Customer {
	int ID;
	String Name;
	public int getID() {
		return ID;
	}
	public void setID(int iD) {
		ID = iD;
	}
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		Name = name;
	}
	public Customer(int iD, String name) {
		super();
		ID = iD;
		Name = name;
	}
	
}
